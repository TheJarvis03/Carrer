{
    "name": "career-guidance",
    "version": "1.0.0",
    "scripts": {
        "prettier": "prettier --single-quote --tab-width 4 --write \"**/*.{js,jsx,json,css,html}\"",
        "start": "concurrently \"npm run server\" \"npm run client\"",
        "server": "cd backend && npm start",
        "client": "cd frontend && npm start",
        "install-all": "npm i && cd backend && npm i && cd ../frontend && npm i",
        "dev": "concurrently \"cd backend && npm run dev\" \"cd frontend && npm start\""
    },
    "devDependencies": {
        "concurrently": "^8.0.1",
        "morgan": "^1.10.0"
    },
    "dependencies": {
        "bcrypt": "^5.1.1",
        "cors": "^2.8.5",
        "express": "^5.1.0",
        "jsonwebtoken": "^9.0.2",
        "mongosh": "^2.5.0",
        "mysql2": "^3.14.0",
        "prettier": "^3.5.3"
    }
}
